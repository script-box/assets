{
  "name": "script-box.assets",
  "version": "0.0.0",
  "private": true,
  "description": "script-box assets.",
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write --plugin-search-dir=. .",
    "githooks": "run-s githooks:*",
    "githooks:01": "husky set .husky/prepare-commit-msg",
    "githooks:02": "husky set .husky/prepare-commit-msg \"exec < /dev/tty && ./node_modules/.bin/cz --hook || true\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-plugin-packagejson": "^2.2.11",
    "wsrun": "^5.2.4"
  }
}
